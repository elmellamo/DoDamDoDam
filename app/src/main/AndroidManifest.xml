<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dodamdodam">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="도담도담"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:enableOnBackInvokedCallback="true"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/Theme.DoDamDoDam">

        <activity
            android:name=".activity.Title"
            android:exported="true"
            android:theme="@style/Theme.DoDamDoDam">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>


        <activity
            android:name=".activity.Login.MainActivity"
            android:exported="true">
        </activity>
        <activity android:name=".activity.Login.LoginActivity"
            android:exported="true">
        </activity>
        <activity android:name=".activity.Login.SignUpActivity"
            android:exported="true">
        </activity>
        <activity android:name=".activity.Login.PasswordResetActivity"
            android:exported="true">
        </activity>
        <activity android:name=".activity.Login.MemberInitActivity"
            android:exported="true">
        </activity>
        <activity android:name=".activity.Login.FindLover"
            android:exported="true">
        </activity>
        <activity android:name=".activity.Login.PutCode"
            android:exported="true">
        </activity>
        <activity android:name=".activity.album.AlbumMain"
            android:exported="true">
        </activity>
        <activity android:name=".activity.album.AlbumAdd"
            android:exported="true">
        </activity>
        <activity android:name=".activity.album.GalleryActivity"
            android:exported="true">
        </activity>
        <activity android:name=".activity.Calendar.CalendarMain"
            android:exported="true">
        </activity>
    </application>

</manifest>